n1 = float(input("¿Cuanto sacó el estudiante en la nota 1? :"))
n2 = float(input("¿Cuanto sacó el estudiante en la nota 2? :"))
n3 = float(input("¿Cuanto sacó el estudiante en la nota 3? :"))
n4 = float(input("¿Cuanto sacó el estudiante en la nota 4? :"))
n5 = float(input("¿Cuanto sacó el estudiante en la nota 5? :"))
resultado = (n1 * 0.15) + (n2 * 0.2) + (n3 * 0.15) + (n4 * 0.3) + (n5 * 0.2)
if resultado < 2:
    print("El estudiante NO PUEDE habilitar la materia")
if resultado < 3:
    print("El estudiante NO APROBO la materia")
if resultado >= 3:
    print("El estudiante APROBÓ la materia")
if resultado > 4.5:
    print("Felicidades por el excelente resultado")
