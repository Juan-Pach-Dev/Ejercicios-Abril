n = int(input("Digite un numero del 1 al 7 :"))
if 1 <= n <= 7:
    if n == 1:
        print("LUNES")
    elif n == 2:
        print("MARTES")
    elif n == 3:
        print("MIERCOLES")
    elif n == 4:
        print("JUEVES")
    elif n == 5:
        print("VIERNES")
    elif n == 6:
        print("SABADO")
    elif n == 7:
        print("DOMINGO")
else:
    print("El numero ingresado no esta en el rengo del 1 al 7, intente de nuevo")
